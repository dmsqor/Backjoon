import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int bit = input.nextInt();
        
        for(int i = 1; i <= bit/4; i++){
            System.out.print("long ");
        }
        System.out.println("int");
        input.close();
    }
}
