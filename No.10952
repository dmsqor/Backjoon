import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int a = 1;
        while(a != 0){
            a = input.nextInt();
            int b = input.nextInt();
            if(a==0) break;
            System.out.println(a+b);
        }
        input.close();
    }
}
